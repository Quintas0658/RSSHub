{
  "framework": null,
  "github": { 
    "silent": true 
  },
  "builds": [
    {
      "src": "api/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CACHE_TYPE": "memory",
    "CACHE_EXPIRE": "600"
  }
}
